SELECT * FROM scan2557 WHERE level LIKE '%ตรี%' and ( education LIKE '%ศิลปกรรม%' or education LIKE '%รัฐป%' or education LIKE '%วิท%' or education LIKE '%นิเทศ%' or education LIKE '%รัฐศ%' or education LIKE '%บริหาร%') and ((chdate32!='' or 'chdate1'!='' or 'chdate2'!='' or chdate3!='' or 'chdate12'!='' or 'chdate22'!='' ) or (chdate32 is not null or 'chdate1'is not null or 'chdate2'is not null or chdate3is not null or 'chdate12'is not null or 'chdate22'is not null )) and type123!='1'ORDER BY scan2557.`count` ASC;